{
  "manifest_version": 3,
  "name": "Gemini Anywhere Assistant",
  "version": "1.1",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://aistudio.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}